<mapper namespace="cn.no7player.mapper.Lable_infoMapper">
    <resultMap id="Lable_infoResultMap" type="cn.no7player.model.dataobject.Lable_infoDO">
        <id column="id" property="id" jdbcType="INTEGER"/>
        <result column="name" property="name" jdbcType="VARCHAR"/>
        <result column="is_hot" property="is_hot" jdbcType="VARCHAR"/>
        <result column="gmt_create" property="gmtCreate" jdbcType="DATE"/>
        <result column="gmt_modified" property="gmtModified" jdbcType="DATE"/>
    </resultMap>

    <insert id="insertLable_info" parameterType="cn.no7player.model.dataobject.Lable_infoDO">
        insert into lable_info (name,is_hot,gmt_create,gmt_modified)
        values (#{name},#{is_hot},#{gmtCreate},#{gmtModified})
    </insert>

    <select id="selectById" resultMap="Lable_infoResultMap" parameterType="java.lang.Long">
        select id,name,is_hot,gmt_create,gmt_modified
        from lable_info
        where id = #{id}
    </select>
</mapper>